<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="FacebookColor">#3b5998</Color>
    <SolidColorBrush x:Key="FacebookColorBrush" Color="{StaticResource FacebookColor}"/>

    <Color x:Key="TwitterColor">#1da1f2</Color>
    <SolidColorBrush x:Key="TwitterColorBrush" Color="{StaticResource TwitterColor}"/>

    <Color x:Key="InstagramColor">#3897f0</Color>
    <SolidColorBrush x:Key="InstagramColorBrush" Color="{StaticResource InstagramColor}"/>

    <Color x:Key="YouTubeColor">#cd201f</Color>
    <SolidColorBrush x:Key="YouTubeColorBrush" Color="{StaticResource YouTubeColor}"/>

    <Color x:Key="GitHubColor">#999999</Color>
    <SolidColorBrush x:Key="GitHubColorBrush" Color="{StaticResource GitHubColor}"/>

    <Color x:Key="PlayStoreColor">#dd4b39</Color>
    <SolidColorBrush x:Key="PlayStoreColorBrush" Color="{StaticResource PlayStoreColor}"/>

</ResourceDictionary>
